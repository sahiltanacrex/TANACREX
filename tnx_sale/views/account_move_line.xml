<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="additional_info_inherit_account_move_line" model="ir.ui.view">
            <field name="name">account.move.form.inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_line_ids']//field[@name='price_subtotal']" position="before">
                    <field name="development_expenses" readonly="1" force_save="1" />
                </xpath>
                <xpath expr="//field[@name='invoice_line_ids']//field[@name='quantity']" position="before">
                    <field name="unit_qty" readonly="1" force_save="1" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>