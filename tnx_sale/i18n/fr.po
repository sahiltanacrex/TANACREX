#. module: tnx_sale
#: model:mail.template,body_html:tnx_sale.email_template_edi_sale_inherit
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        ATTN :\n"
"                        <t t-if=\"object.partner_id.parent_id\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        <t t-set=\"doc_name\" t-value=\"'quotation' if object.state in ('draft', 'sent') else 'order'\"/>\n"
"                        Dear Sir/Madam,\n"
"                        <br/><br/>\n"
"                        Please find enclosed the\n"
"                        <t t-if=\"ctx.get('proforma')\">\n"
"                            PROFORMAT invoice <t t-out=\"doc_name or ''\"/> <strong t-out=\"object.name or ''\">S00052</strong>\n"
"                            <t t-if=\"object.sale_order_partner\">\n"
"                                (reference: <t t-out=\"object.sale_order_partner or ''\"/> )\n"
"                            </t>\n"
"                            amounting to a total of <strong t-out=\"format_amount(object.amount_total, object.pricelist_id.currency_id) or ''\">$ 10.00</strong>.\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"doc_name or ''\">quotation</t> <strong t-out=\"object.name or ''\"/>\n"
"                            <t t-if=\"object.sale_order_partner\">\n"
"                                (reference: <t t-out=\"object.sale_order_partner or ''\">S00052</t> )\n"
"                            </t>\n"
"                            amounting to a total of <strong t-out=\"format_amount(object.amount_total, object.pricelist_id.currency_id) or ''\">$ 10.00</strong>.\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        We look forward to receiving your approval and confirmation,\n"
"                        <br/><br/>\n"
"                        Best Regards,\n"
"                        <br/><br/>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        ATTN :\n"
"                        <t t-if=\"object.partner_id.parent_id\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        <t t-set=\"doc_name\" t-value=\"'quotation' if object.state in ('draft', 'sent') else 'order'\"/>\n"
"                        Bonjour,\n"
"                        <br/><br/>\n"
"                        Nous vous prions de bien vouloir trouver ci-joint\n"
"                        <t t-if=\"ctx.get('proforma')\">\n"
"                            la facture PROFORMA <t t-out=\"doc_name or ''\"/> <strong t-out=\"object.name or ''\">S00052</strong>\n"
"                            <t t-if=\"object.sale_order_partner\">\n"
"                                (suivant référence: <t t-out=\"object.sale_order_partner or ''\"/> )\n"
"                            </t>\n"
"                            amounting to a total of <strong t-out=\"format_amount(object.amount_total, object.pricelist_id.currency_id) or ''\">$ 10.00</strong>.\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"doc_name or ''\">le devis</t> <strong t-out=\"object.name or ''\"/>\n"
"                            <t t-if=\"object.sale_order_partner\">\n"
"                                (suivant référence: <t t-out=\"object.sale_order_partner or ''\">S00052</t> )\n"
"                            </t>\n"
"                            d’un montant de <strong t-out=\"format_amount(object.amount_total, object.pricelist_id.currency_id) or ''\">$ 10.00</strong>.\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Dans l’attente de votre retour et validation,\n"
"                        <br/><br/>\n"
"                        Bien Cordialement,\n"
"                        <br/><br/>\n"
"                    </p>\n"
"                </div>\n"
"            "

