msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        ATTN :\n"
"                        <t t-if=\"object.partner_id.parent_id\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"                        </t>\n"
"                        <br/><br/>\n"
"                        Dear Sir/Madam,\n"
"                        <br/><br/>\n"
"                        Please find attached the\n"
"                        <t t-if=\"object.name\">\n"
"                            invoice <strong t-out=\"object.name or ''\">INV/2021/05/0005</strong>\n"
"                        </t>\n"
"                        <t t-else=\"\">\n"
"                            invoice\n"
"                        </t>\n"
"                        <t t-if=\"object.invoice_origin\">\n"
"                            (with reference: <t t-out=\"object.invoice_origin or ''\">SUB003</t>)\n"
"                        </t>\n"
"                        amounting to a total of <strong t-out=\"format_amount(object.amount_total, object.currency_id) or ''\">$ 143,750.00</strong>\n"
"\n"
"                        . The invoice is due by : <strong t-out=\"object.invoice_date_due.strftime('%d/%m/%Y') or ''\"/>\n"
"                        <br/><br/>\n"
"                        We look forward to hearing from you soon,\n"
"                        <br/><br/>\n"
"                        Best Regards,\n"
"                        <t t-if=\"object.invoice_user_id.signature\">\n"
"                            <br/>\n"
"                            <t t-out=\"object.invoice_user_id.signature or ''\">--<br/>Mitchell Admin</t>\n"
"                        </t>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
        "<div style=\"margin: 0px; padding: 0px;\">\n"
        "                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
        "                        ATTN :\n"
        "                        <t t-if=\"object.partner_id.parent_id\">\n"
        "                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>),\n"
        "                        </t>\n"
        "                        <t t-else=\"\">\n"
        "                            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
        "                        </t>\n"
        "                        <br/><br/>\n"
        "                        Bonjour,\n"
        "                        <br/><br/>\n"
        "                        Nous vous prions de bien vouloir trouver ci-joint la \n"
        "                        <t t-if=\"object.name\">\n"
        "                            facture <strong t-out=\"object.name or ''\">INV/2021/05/0005</strong>\n"
        "                        </t>\n"
        "                        <t t-else=\"\">\n"
        "                            facture\n"
        "                        </t>\n"
        "                        <t t-if=\"object.invoice_origin\">\n"
        "                            (suivant la référence: <t t-out=\"object.invoice_origin or ''\">SUB003</t>)\n"
        "                        </t>\n"
        "                        d’un montant de <strong t-out=\"format_amount(object.amount_total, object.currency_id) or ''\">$ 143,750.00</strong>\n"
        "\n"
        "                        , payable avant le : <strong t-out=\"object.invoice_date_due.strftime('%d/%m/%Y') or ''\"/>\n"
        "                        <br/><br/>\n"
        "                        En vous souhaitant une bonne réception.\n"
        "                        <br/><br/>\n"
        "                        Bien Cordialement,\n"
        "                        <t t-if=\"object.invoice_user_id.signature\">\n"
        "                            <br/>\n"
        "                            <t t-out=\"object.invoice_user_id.signature or ''\">--<br/>Mitchell Admin</t>\n"
        "                        </t>\n"
        "                    </p>\n"
        "                </div>\n"
