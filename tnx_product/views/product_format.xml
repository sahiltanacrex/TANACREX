<odoo>
    <data>
        <record id="view_product_format_view" model="ir.ui.view">
            <field name="name">mds_form.form</field>
            <field name="model">product.format</field>
            <field name="arch" type="xml">
                <form string="Price rental" >
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>

                    </sheet>
<!--                    <div class="oe_chatter">-->
<!--                        <field name="message_follower_ids" widget="mail_followers"/>-->
<!--                        <field name="message_ids" widget="mail_thread"/>-->
<!--                    </div>-->
                </form>
            </field>
        </record>
        <record id="view_product_format_tree" model="ir.ui.view">
            <field name="name">product_format.tree</field>
            <field name="model">product.format</field>
            <field name="arch" type="xml">
                <tree string="Format product">
                    <field name="name"/>      
                </tree>
            </field>
        </record>
    
        <record model="ir.actions.act_window" id="action_product_format">
            <field name="name">Format product</field>
            <field name="res_model">product.format</field>
            <field name="view_mode">tree,form</field>
            <!-- <field name="context">{"search_default_sector_id":1,"search_default_directory_id":1}</field> -->
        </record>
        <menuitem id="product_format_menu" name="Product format"  action="action_product_format" parent="stock.menu_stock_config_settings"/>
    </data>
</odoo>